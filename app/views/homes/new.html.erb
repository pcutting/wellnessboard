<h1>New home</h1>

<% form_for(@home) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :option %><br />
    <%= f.text_field :option %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', homes_path %>