<h1>Listing catalogs</h1>

<table>
  <tr>
    <th>Sku</th>
    <th>Item</th>
    <th>Description</th>
    <th>Picture</th>
    <th>Price</th>
    <th>Category</th>
  </tr>

<% @catalogs.each do |catalog| %>
  <tr>
    <td><%=h catalog.sku %></td>
    <td><%=h catalog.item %></td>
    <td><%=h catalog.description %></td>
    <td><%=h catalog.picture %></td>
    <td><%=h catalog.price %></td>
    <td><%=h catalog.category %></td>
    <td><%= link_to 'Show', catalog %></td>
    <td><%= link_to 'Edit', edit_catalog_path(catalog) %></td>
    <td><%= link_to 'Destroy', catalog, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New catalog', new_catalog_path %>