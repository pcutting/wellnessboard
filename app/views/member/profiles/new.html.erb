<h1>New profile</h1>

<% form_for(@profile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :shipping_address %><br />
    <%= f.text_area :shipping_address %>
  </p>
  <p>
    <%= f.label :physical_address %><br />
    <%= f.text_area :physical_address %>
  </p>
  <p>
    <%= f.label :sponsor %><br />
    <%= f.text_field :sponsor %>
  </p>
  <p>
    <%= f.label :coach %><br />
    <%= f.text_field :coach %>
  </p>
  <p>
    <%= f.label :referred_by %><br />
    <%= f.text_field :referred_by %>
  </p>
  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :alt_phone %><br />
    <%= f.text_field :alt_phone %>
  </p>
  <p>
    <%= f.label :hight %><br />
    <%= f.text_field :hight %>
  </p>
  <p>
    <%= f.label :time_zone %><br />
    <%= f.text_field :time_zone %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', profiles_path %>